{
  "info": {
    "name"      : "{{name}}",
    "author"    : "{{author}}",
    "copyright" : "{{copyright}}",
    "license"   : "{{license}}",
    "version"   : "{{version}}",
    "class_name": "{{class_name}}",
    "file_name" : "{{file_name}}",
    "inputs"    : {{inputs}},
    "outputs"   : {{outputs}}
  },

  "active": {
    {% for w in active %}"{{w.label}}": { "var": "{{w.var}}", "type": "{{w.type}}", "init": {{w.init}}, "min": {{w.min}}, "max": {{w.max}} }{% if not loop.last %},
    {% endif %}{% endfor %}
  },

  "passive": {
    {% for w in passive %}"{{w.label}}": { "var": "{{w.var}}", "type": "{{w.type}}", "init": {{w.init}}, "min": {{w.min}}, "max": {{w.max}} }{% if not loop.last %},
    {% endif %}{% endfor %}
  }
}
